{
  "name": "vc-frontend-static",
  "version": "0.0.1",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,vue}\"",
    "lint:formatting": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md,vue}\""
  },
  "dependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.0",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.2.6",
    "tailwindcss": "^2.1.4",
    "vite": "^2.3.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md,vue}": [
      "prettier --write"
    ]
  }
}
