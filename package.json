{
  "name": "vc-frontend-static",
  "version": "0.0.1",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "verbose": true
  },
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,vue}\" \"__tests__/**/*.{js,jsx,ts,tsx,json,md,vue}\"",
    "lint:formatting": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md,vue}\" \"__tests__/**/*.{js,jsx,ts,tsx,json,md,vue}\"",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "axios": "^0.21.1",
    "babel-jest": "^26.6.3",
    "botframework-directlinejs": "^0.14.1",
    "focus-trap": "^6.6.0",
    "focus-trap-vue": "^3.2.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.0",
    "vue": "^3.1.2",
    "vue-i18n": "^9.1.6",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^2.3.1",
    "@tailwindcss/forms": "^0.3.3",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.2",
    "@vue/test-utils": "^2.0.0-rc.10",
    "autoprefixer": "^10.2.6",
    "focus-visible": "^5.2.0",
    "postcss-focus-visible": "^5.0.0",
    "tailwindcss": "^2.2.4",
    "vite": "^2.3.8"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md,vue}": [
      "prettier --write"
    ],
    "__tests__/**/*.{js,jsx,ts,tsx,json,md,vue}": [
      "prettier --write"
    ]
  }
}
